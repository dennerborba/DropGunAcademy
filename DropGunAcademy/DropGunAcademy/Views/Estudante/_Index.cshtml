@using DropGunAcademy.Models.Estudantes
<div>
    <h1>Aluno DropGunAcademy</h1>
    <table class="table table-striped">
        <thead>
            <tr>
                <th>ID</th>
                <th>Nome</th>
                <th>Cpf</th>
                <th>Data de Nascimento</th>
                <th>Endereço</th>
                <th>Telefone</th>
                <th>E-mail</th>
                <th>Status</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var estudante in Model.Estudante)
            {
                <tr>
                    <td>@estudante.id</td>
                    <td>@estudante.nome</td>
                    <td>@estudante.cpf</td>
                    <td>@estudante.dataNascimento</td>
                    <td>@estudante.endereco</td>
                    <td>@estudante.telefone</td>
                    <td>@estudante.email</td>
                    <td>@estudante.status</td>
                    <td><a type="button" class="btn btn-primary" asp-controller="Carros" asp-action="Editar" asp-route-id="@estudante.id">Editar</a></td>
                </tr>
            }
        </tbody>
    </table>
</div>
